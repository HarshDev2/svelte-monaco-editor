{
	"name": "svelte-monaco-editor",
	"version": "1.0.0",
	"description": "An very component to intergrate monaco editor in your svelte application.",
	"keywords": ["svlte monaco editor", "monaco-editor", "svelte code editor"],
	"repository": {
		"type": "git",
		"url": "https://github.com/HarshDev2/svelte-monaco-editor.git"
	},
	"author": {
		"name": "Harsh Agrawal",
		"url": "https://harshagrawal.xyz",
		"email": "harshagrawal2489@gmail.com"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"monaco-editor": "^0.38.0"
	}
}
